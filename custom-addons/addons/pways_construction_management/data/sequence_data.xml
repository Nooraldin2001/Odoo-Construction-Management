        </record>

        <record id="purchase_requisition_seq" model="ir.sequence">
            <field name="name">Purchase Requisition Sequence</field>
            <field name="code">purchase.requisition.seq</field>
            <field name="prefix">EPR</field>
            <field name="padding">5</field>
        </record>

        <record id="vehicle_equipment_request_seq" model="ir.sequence">
            <field name="name">Vehicle Equipment Request</field>
            <field name="code">vehicle.equipment.request</field>
            <field name="padding">5</field>
        </record>
